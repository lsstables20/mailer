{
    "name": "twenty20/mailer",
    "description": "A multi-provider mailer package for SendGrid, Amazon SES, Mailchimp, Mailgun.",
    "keywords": [
        "Twenty20",
        "laravel",
        "mailer"
    ],
    "homepage": "https://github.com/twenty20/mailer",
    "license": "MIT",
    "authors": [
        {
            "name": "Twenty20",
            "email": "dev@twenty20.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^9.0||^10.0||^11.0",
        "illuminate/http": "^9.0||^10.0||^11.0",
        "illuminate/support": "^9.0||^10.0||^11.0",
        "laravel/prompts": "*",
        "mailgun/mailgun-php": "^4.3",
        "sendgrid/sendgrid": "^8.1",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9||^3.0",
        "orchestra/testbench": "^8.22||^9.0||^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^2.0||^3.0"
    },
    "autoload": {
        "psr-4": {
            "Twenty20\\Mailer\\": "src/",
            "Twenty20\\Mailer\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Twenty20\\Mailer\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Twenty20\\Mailer\\MailerServiceProvider"
            ],
            "aliases": {
                "Mailer": "Twenty20\\Mailer\\Facades\\Mailer"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
